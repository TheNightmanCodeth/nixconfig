{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
		"rosewater": "#f4dbd6",
		"flamingo": "#f0c6c6",
		"pink": "13",
		"mauve": "#c6a0f6",
		"red": "9",
		"maroon": "#ee99a0",
		"peach": "#f5a97f",
		"yellow": "11",
		"green": "10",
		"teal": "14",
		"sky": "#91d7e3",
		"sapphire": "#7dc4e4",
		"lavender": "#b7bdf8",
		"blue": "12",
		"text": "#cad3f5",
		"subtext1": "7",
		"subtext0": "15",
		"overlay2": "#939ab7",
		"overlay1": "#8087a2",
		"overlay0": "#6e738d",
		"surface2": "8",
		"surface1": "0",
		"surface0": "#363a4f",
		"base": "#24273a",
		"mantle": "#1e2030",
		"crust": "#181926",

		"bad": "p:red",
		"warn": "p:yellow",
		"good": "p:green",

		"bg1": "p:teal",
		"fg1": "p:surface1",

		"bg2": "p:blue",
		"fg2": "p:surface1",

		"bg3": "p:teal",
		"fg3": "p:surface1",

		"bg4": "p:blue",
		"fg4": "p:surface1",

		"bg5": "p:teal",
		"fg5": "p:surface1",

		"bg6": "p:pink",
		"fg6": "p:surface1",

		"bg7": "p:blue",
		"fg7": "p:surface1"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:bg1",
          "foreground": "p:fg1",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "windows": "\uf179"
          },
          "style": "powerline",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "p:bg1",
          "foreground": "p:fg1",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:bg2",
          "foreground": "p:fg2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "powerline",
          "template": " \ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:bg3",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:warn{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:warn{{ end }}",
            "{{ if gt .Ahead 0 }}p:warn{{ end }}",
            "{{ if gt .Behind 0 }}p:warn{{ end }}"
          ],
          "foreground": "p:fg3",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "powerline",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:bg5",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:warn{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:warn{{end}}",
            "{{if eq \"Full\" .State.String}}p:good{{end}}"
          ],
          "foreground": "p:fg5",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}\uf295 ",
          "properties": {
            "discharging_icon": " ",
            "charging_icon": " ",
            "charged_icon": " "
          },
          "type": "battery"
        },
        {
          "background": "p:bg6",
          "foreground": "p:fg6",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": "  {{ .Full }} ",
          "type": "swift"
        },
        {
          "background": "p:bg6",
          "foreground": "p:fg6",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": "  {{ .Full }} ",
          "type": "rust"
        },
        {
          "background": "p:bg6",
          "foreground": "p:fg6",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": "  {{ .Full }} ",
          "type": "elixir"
        },
        {
          "background": "p:bg7",
          "foreground": "p:fg7",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": "<#fff>\ue641 </>{{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:green",
          "style": "plain",
          "template": "\uf105",
          "type": "root"
        },
        {
          "foreground": "p:teal",
          "style": "plain",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:text",
    "template": "\uf105 "
  },
  "version": 1
}
